{
  "name": "@ruijadom/telescope-vite",
  "version": "0.1.1",
  "description": "Vite plugin for Telescope integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruijadom/telescope.git",
    "directory": "packages/vite-plugin"
  },
  "author": "Rui Jadom <ruijadom@gmail.com>",
  "license": "MIT",
  "keywords": [
    "telescope",
    "vite",
    "plugin",
    "developer-tools",
    "devtools"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ruijadom/telescope-core": "*"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0",
    "typescript": "^5.0.0"
  }
}
